    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "brotli-webpack-plugin": "^1.1.0",
    "chai": "^4.2.0",
    "chokidar": "^3.4.2",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "electron": "^11.2.0",
    "esbuild-loader": "^2.4.0",
    "eslint": "^7.6.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.5",
    "eslint-watch": "^7.0.0",
    "file-loader": "^4.2.0",
